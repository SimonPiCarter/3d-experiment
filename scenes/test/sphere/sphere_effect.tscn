	vec4 n_out9p0 = fma(vec4(n_out7p0), n_out6p0, n_in9p2);


// VectorOp:10
	vec4 n_out10p0 = n_out8p0 + n_out9p0;


// VectorDecompose:11
	float n_out11p0 = n_out10p0.x;
	float n_out11p1 = n_out10p0.y;
	float n_out11p2 = n_out10p0.z;
	float n_out11p3 = n_out10p0.w;


// Output:0
	ALBEDO = vec3(n_out10p0.xyz);
	ALPHA = n_out11p3;


}
